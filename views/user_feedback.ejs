<%- include('partials/main')%>
    <div id="content-container">	
          <div id="page-title">
              <script>
                  $( function() {
                    $( "#dialog" ).dialog();
                  } );
                  </script>
              <div id="dialog" title="Basic dialog" class="modal">
                  <div class="modal-dialog" role="dialog">
                      <div class="modal-content">
                          <div class="modal-header">
                              <h5 class="modal-title" id="exampleModalLabel">Add Coupon</h5>
                                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                  <span aria-hidden="true">&times;</span>
                                  </button>
                          </div>
                          <div class="modal-body">
                              <form action="/location" class="form-horizontal" method="post" id="coupon_add" enctype="multipart/form-data" accept-charset="utf-8">
                                  <div class="panel-body">
                                      <div class="form-group">
                                          <label class="col-sm-4 control-label" for="demo-hor-1">Add Location</label>
                                              <div class="col-sm-6">
                                                  <input name="title" id="addinput" 
                                                  placeholder="Title" class="form-control required" type="text">
                                          </div>
                                      </div>
                                  </div>
                              </form>
                          </div>
                          <div class="modal-footer">
                              <div class="modal-footer"> 
                                  <button type="button" id="dismisaddmodel" class="btn btn-secondary" data-dismiss="modal">
                                      Close
                                  </button>
                                  <Button type="submit" id= "addloc" class="btn btn-primary" >
                                          Save 
                                  </Button>
                              </div>
                          </div>
                      </div>
                  </div>  
              </div>
                 
              <h1 class="page-header text-overflow">Dashboard</h1>
          </div>
            <div class="table-responsive">
          <table class="table table-striped table-bordered table-hover" style="font-size:16px;">
            <thead class="thead-dark">
              <tr>
                <th scope="col">Id</th>
                <th scope="col">Name </th>
                <th scope="col">Feedback Type</th>
                <th scope="col">Feedback Date</th>
                <th scope="col">Order No</th>
                <th scope="col">Calling Time</th>
                <th scope="col">Message</th>
              </tr>
            </thead>
            <tbody>
                <% newarray.forEach(function(loc,index) { %>
              <tr>
                <th scope="row"><%= index+1 %></th>
                <td><%= loc.user %></td>
                <td><%= loc.feedback_type  %></td>
                <td><%= loc.feedback_date  %></td>
                <td><%= loc.order_no  %></td>
                <td><%= loc.callingtime  %></td>
                <td><%= loc.message  %></td>
              </tr>
              <% }); %>
            </tbody>
          </table>
        </div>
        </div>        
       
<%- include('partials/allbottom.ejs') %>
 