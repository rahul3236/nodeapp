<%- include('partials/main')%>
    

          
              <div id="content-container">	
                <div id="page-title">
                    <h1 class="page-header text-overflow">Dashboard</h1>
                </div>
                
                <div class="tab-base">
                      <div class="panel">
                          <div class="panel-body">
                              <div class="tab-content">
                                  <div style="border-bottom: 1px solid #ebebeb;padding: 25px 5px 5px 5px;" class="col-md-12">
                      <div class="bootstrap-table">
                          <div class="fixed-table-toolbar">
                              <div class="columns columns-right btn-group pull-right">
                                     
                                  </div>
                      
                      <table id="demo-table" class="table table-striped table-hover">
              
                          <thead>
                                  
                              <tr><th style=""><div class="th-inner ">No</div><div class="fht-cell"></div></th><th style=""><div class="th-inner ">Name</div><div class="fht-cell"></div></th><th style=""><div class="th-inner ">Feedback Type</div><div class="fht-cell"></div></th><th class="text-right" style=""><div class="th-inner ">Feedback Date</div><div class="fht-cell"></div></th>
                                <th class="text-right" style=""><div class="th-inner ">Oreder Number</div><div class="fht-cell"></div></th>
                                <th class="text-right" style=""><div class="th-inner ">Calling Type</div><div class="fht-cell"></div></th>
                                <th class="text-right" style=""><div class="th-inner ">Message</div><div class="fht-cell"></div></th>
                            </tr>
                          </thead>
                              
                          <tbody>
                                  <% newarray.forEach(function(loc,index) { %>
                              <tr>
                                  <td><%=  index+1  %>
                                  </td>
                                  <td><%= loc.user %></td>
                                  <td><%= loc.feedback_type  %></td>
                                  <td><%= loc.feedback_date  %></td>
                                  <td><%= loc.order_no  %></td>
                                  <td><%= loc.callingtime  %></td>
                                  <td><%= loc.message  %></td>
                                </tr>
                                <% }); %>
                                              </tbody>
                      </table>
                  </div>
                  <div class="fixed-table-pagination">
            
                    <div class="pull-right pagination">
                        <ul class="pagination">
                            <li class="page-first disabled">
                                <a href="javascript:void(0)">&lt;&lt;</a></li><li class="page-pre disabled"><a href="javascript:void(0)">&lt;</a></li><li class="page-number active disabled"><a href="javascript:void(0)">1</a></li><li class="page-number"><a href="javascript:void(0)">2</a></li><li class="page-number"><a href="javascript:void(0)">3</a></li><li class="page-number"><a href="javascript:void(0)">4</a></li><li class="page-number"><a href="javascript:void(0)">5</a></li><li class="page-next"><a href="javascript:void(0)">&gt;</a></li><li class="page-last"><a href="javascript:void(0)">&gt;&gt;</a></li></ul></div></div><div class="clearfix"></div>
          
              </div>
                              </div>
                          </div>
                      </div>
                  </div>          
      </div> 



<script>
    $(document).ready(function(){
        $(".page-number").on('click', function(){
            let temp= $(this).val()
            window.location.href="http://localhost:5000/userfeedback/" + temp
    
        })
        })
</script>


